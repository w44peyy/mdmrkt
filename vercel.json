{
  "buildCommand": "echo 'No build required'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/mardinlee/admin.html"
    },
    {
      "source": "/mardinlee",
      "destination": "/mardinlee/admin.html"
    },
    {
      "source": "/api/purchases",
      "destination": "/mardinlee/api/purchases.js"
    },
    {
      "source": "/api/stats",
      "destination": "/mardinlee/api/stats.js"
    },
    {
      "source": "/api/online-users",
      "destination": "/mardinlee/api/online-users.js"
    },
    {
      "source": "/api/activity",
      "destination": "/mardinlee/api/activity.js"
    },
    {
      "source": "/mardinlee/api/activity",
      "destination": "/mardinlee/api/activity.js"
    },
    {
      "source": "/api/phones",
      "destination": "/mardinlee/api/phones.js"
    },
    {
      "source": "/api/products",
      "destination": "/mardinlee/api/products.js"
    },
    {
      "source": "/api/visitors",
      "destination": "/mardinlee/api/visitors.js"
    },
        {
          "source": "/api/heartbeat",
          "destination": "/mardinlee/api/heartbeat.js"
        },
        {
          "source": "/api/init",
          "destination": "/api/init.js"
        },
        {
          "source": "/api/cart",
          "destination": "/api/cart.js"
        },
        {
          "source": "/api/checkout",
          "destination": "/api/checkout.js"
        },
        {
          "source": "/mardinlee/api/checkout",
          "destination": "/api/checkout.js"
        }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    },
    {
      "source": "/mardinlee/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}

